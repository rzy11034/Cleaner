program Clean;

{$mode objfpc}{$H+}
{$ModeSwitch unicodestrings}

uses
 {$IFDEF UNIX}
  cthreads,
  cwstring,
 {$ENDIF}
  SysUtils,
  Clean.Utils,
  Clean.Main;

var
  Cln: TClean;

begin
  Cln := TClean.Create;
  try
    try
      Cln.Run;
    except
      on E: Exception do
        Writeln(E.ClassName, ': ', E.Message);
    end;
  finally
    FreeAndNil(Cln);
  end;
end.
